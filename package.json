{
  "name": "queso",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "api": "node server-node/src/node_api_server.js",
  "dev": "concurrently -k -n api,web -c green,cyan \"npm:api\" \"vite\"",
  "lab:dev": "concurrently -k -n api,lab -c green,magenta \"npm:api\" \"npm:lab:client\"",
  "lab:install-prereqs": "pwsh -NoProfile -ExecutionPolicy Bypass -File server-node/scripts/install_lab_prereqs.ps1",
  "lab:smoke": "node server-node/scripts/lab_smoke_test.js",
  "lab:rot": "node server-node/scripts/generate_files_rot.js",
  "lab:client": "vite --config client/lab/vite.config.js --port 5174",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "svelte": "^4.2.18",
    "vite": "^5.4.8"
  }
}
